{
  "name": "<%= conf.get('themeDir') %>",
  "textdomain": "<%= conf.get('themeDir') %>",
  "themeName": "<% print(conf.get('themeDir').replace(/-/g, ' ').replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();})); %>",
  "functionPrefix": "<% print(conf.get('themeDir').replace(/-/g, '')); %>",
  "classPrefix": "<% print(conf.get('themeDir').replace(/-/g, ' ').replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();}).replace(/\s/g, '')); %>",
  "constantPrefix": "<% print(conf.get('themeDir').replace(/-/g, '').toUpperCase()); %>",
  "version": "1.0.0",
  "description": "Custom wordpress theme. Based on _undersores and bootstrap, developed with Yoeman and Grunt",
  "keywords": [
    "wordpress",
    "grunt",
    "starter-theme",
    "bootstrap"
  ],
  "author": {
    "name": "Daniel Hernandez",
    "url": "http://dh-creative-webdesign.de/"
  },
  "license": {
    "type": "GPL-2.0",
    "name": "GNU General Public License v2",
    "url": "http://www.gnu.org/licenses/gpl-2.0.html"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-imagemin": "^0.9.3",
    "grunt-contrib-uglify": "~0.7.0",
    "grunt-contrib-sass": "~0.7.4",
    "grunt-banner": "~0.3.1",
    "grunt-text-replace": "~0.4.0",
    "grunt-newer" : "~1.1.2",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-delete-sync": "~0",
    "autoprefixer": "~6.0.0",
    "cssnano": "~2.0.0",
    "grunt-postcss": "~0.8.0",
    "grunt-browser-sync": "^1.5.3",
    "grunt-npm-install": "~0.3.1"
  }
}